{
  "name": "@uniformdev/canvas-next",
  "version": "19.13.0",
  "description": "Next.js SDK for Uniform Canvas",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.js"
    },
    "./slug": {
      "import": {
        "node": "./dist/slug/index.mjs",
        "default": "./dist/slug/index.esm.js"
      },
      "require": "./dist/slug/index.js"
    },
    "./project-map": {
      "import": {
        "node": "./dist/project-map/index.mjs",
        "default": "./dist/project-map/index.esm.js"
      },
      "require": "./dist/project-map/index.js"
    },
    "./route": {
      "import": {
        "node": "./dist/route/index.mjs",
        "default": "./dist/route/index.esm.js"
      },
      "require": "./dist/route/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "slug": [
        "./dist/slug/index.d.ts"
      ],
      "project-map": [
        "./dist/project-map/index.d.ts"
      ],
      "route": [
        "./dist/route/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1 --passWithNoTests",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "document": "api-extractor run --local"
  },
  "dependencies": {
    "@uniformdev/canvas": "19.13.0",
    "@uniformdev/canvas-react": "19.13.0",
    "@uniformdev/project-map": "19.13.0",
    "@uniformdev/redirect": "19.13.0",
    "ansi-colors": "^4.1.3"
  },
  "peerDependencies": {
    "next": ">= 12.0.0",
    "react": ">= 16 || 17 || 18",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1670a0e2475a7a98745ea737cd3b2b09649fe1a1"
}
